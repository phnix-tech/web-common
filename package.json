{
  "name": "owinfo-web-common",
  "version": "1.0.0",
  "description": "前端公共仓库，包含文档说明、公共库函数、公共组件等",
  "author": "Jefy.Lee<lizhihui@owinfo.net>",
  "private": true,
  "scripts": {
    "lint": "run-p eslint stylelint",
    "eslint": "eslint --config ./eslintrc.js --cache --cache-location ./build/lint/.eslintcache --ignore-pattern \"/doc/**\" --ignore-pattern \"/src/vue/**\" --ext .js ./",
    "stylelint": "stylelint --cache --cache-location ./build/lint/.stylelintcache \"./**/*.scss\" \"./**/*.less\" \"./**/*.vue\""
  },
  "dependencies": {
    "vue": "2.6.10",
    "axios": "0.19.0"
  },
  "devDependencies": {
