    "react-redux": "7.2.1"
  },
  "devDependencies": {
    "vue-template-compiler": "2.6.10",
    "typescript": "3.9.4",
    "@types/react": "16.9.51",
    "@types/react-redux": "7.1.9",
    "npm-run-all": "4.1.5",
    "@web-io/build": "0.2.3",
    "@web-io/lint": "0.4.1",
    "eslint": "6.8.0",
    "stylelint": "9.10.1",
    "babel-eslint": "10.0.3",
    "@typescript-eslint/parser": "4.4.1",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "husky": "4.2.1",
    "lint-staged": "10.0.7"
