{
  "name": "@web/common",
  "version": "1.0.5",
  "description": "前端公共仓库，包含文档说明、公共库函数、公共组件等",
  "author": "leejefy <050934@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "run-p eslint eslint-ts stylelint",
    "eslint": "eslint --config ./eslintrc.js --cache --cache-location ./build/lint/.eslintcache --ignore-pattern \"/src/vue/**\" --ext .js ./",
    "eslint-ts": "eslint --config ./eslintrc-ts.js --cache --cache-location ./build/lint/.eslinttscache --ext .ts ./",
    "stylelint": "stylelint --cache --cache-location ./build/lint/.stylelintcache \"./**/*.scss\" \"./**/*.less\" \"./**/*.vue\""
  },
  "dependencies": {
    "vue": "2.6.10",
    "vuex": "3.5.1",
    "react": "16.13.1",
