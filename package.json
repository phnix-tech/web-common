    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "node-sass": "4.13.0",
    "sass-loader": "8.0.0",
    "vue-template-compiler": "2.6.10",
    "npm-run-all": "4.1.5",
    "fs-extra": "8.1.0",
    "gs-common": "0.2.1",
    "gs-lint": "0.4.0",
    "eslint": "6.8.0",
    "stylelint": "9.10.1",
    "babel-eslint": "10.0.3"
  },
  "license": "MIT",
  "browserslist": [
    "> 1%",
